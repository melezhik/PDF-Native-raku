all :
	(cd src/pdf && $(MAKE) all)

test-coverage :
	export COVERAGE=1;\
	$(MAKE) clean test;\
	cd src/pdf && $(MAKE) coverage-reports

test : all
	@prove -e'perl6 -I lib' -v t

memtest : all
	@prove -e'perl6-valgrind-m -I lib' -v t

clean :
	(cd src/pdf && $(MAKE) $@)

realclean :
	(cd src/pdf && $(MAKE) $@)
	@rm -f Makefile

